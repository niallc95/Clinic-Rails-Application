<div class="container">
	<div class="edit-report-form">
		<center><p class="new_patient_text">Edit Report</p></center>
		<%= simple_form_for([@patient, @report]) do |f| %>
			<%= f.input :date, :input_html => {:style => 'width: 250px;height:50px;'} %>
			<%= f.input :report, :input_html => {:style => 'height:320px;'}%>
			<center><%= f.button :submit, :class => 'button_one'  %></center>
	<% end %>
</div>